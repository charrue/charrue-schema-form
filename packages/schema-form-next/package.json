{
  "name": "@charrue/schema-form-next",
  "version": "0.2.3",
  "description": "基于vue3与element-plus的表单组件封装",
  "keywords": [
    "charrue",
    "element-plus",
    "form"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charrue/charrue-schema-form",
    "directory": "packages/schema-form-next"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "typings": "typings/index.d.ts",
  "files": [
    "dist",
    "typings",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:component && npm run build:types",
    "build:component": "vue3-bundler",
    "build:types": "rm -rf ./typings && vue-tsc --declaration --emitDeclarationOnly"
  },
  "devDependencies": {
    "@charrue/vue3-bundler": "^0.1.0",
    "vue": "3.2.25"
  },
  "peerDependencies": {
    "element-plus": "*",
    "vue": "3"
  },
  "publishConfig": {
    "access": "public"
  }
}