{
  "name": "@charrue/schema-form-next",
  "version": "1.2.0",
  "description": "基于vue3与element-plus的表单组件封装",
  "keywords": [
    "charrue",
    "element-plus",
    "form"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charrue/charrue-schema-form",
    "directory": "packages/schema-form-next"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "jsdelivr": "dist/index.full.js",
  "unpkg": "dist/index.full.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "style": "index.css",
  "files": [
    "dist",
    "index.css"
  ],
  "scripts": {
    "build": "tsup && npm run build:full && npm run build:style",
    "dev": "tsup src/index.ts --no-config --watch --dts --format esm,cjs --splitting",
    "build:full": "rollup -c",
    "build:style": "gulp --gulpfile gulpfile.js default",
    "dev:style": "gulp --gulpfile gulpfile.js dev",
    "test": "vitest"
  },
  "dependencies": {
    "element-plus": "^2.2.18",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/lodash.isequal": "^4.5.6",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^5.1.0",
    "lodash.isequal": "^4.5.0",
    "rollup": "^3.14.0",
    "rollup-plugin-esbuild": "^5.0.0"
  },
  "peerDependencies": {
    "element-plus": "*",
    "vue": "3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}